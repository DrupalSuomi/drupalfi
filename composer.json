{
    "name": "drupal-fi/drupal-fi",
    "description": "Drupal Finland website",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "desandro/masonry": "4.2.2",
        "dinbror/blazy": "^1.8.2",
        "drupal/addtoany": "^1.12",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/allowed_formats": "^1.3",
        "drupal/blazy": "^2.0@rc",
        "drupal/components": "^2.0",
        "drupal/config_readonly": "^1.0@beta",
        "drupal/content_browser": "^1.0@alpha",
        "drupal/cookieconsent": "^1.1",
        "drupal/core-composer-scaffold": "~9.1.0",
        "drupal/core-project-message": "~9.1.0",
        "drupal/core-recommended": "~9.1.0",
        "drupal/crop": "~2.0",
        "drupal/devel": "^4.0",
        "drupal/devel_entity_updates": "^3.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_browser": "^2.2",
        "drupal/field_group": "~3.0",
        "drupal/focal_point": "^1.0@beta",
        "drupal/google_tag": "^1.1",
        "drupal/honeypot": "^2.0.1",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/language_switcher_extended": "^1.0@beta",
        "drupal/leaflet": "^2.0",
        "drupal/linkit": "^5.0@beta",
        "drupal/media_entity_browser": "^2.0@alpha",
        "drupal/metatag": "^1.0@beta",
        "drupal/paragraph_view_mode": "^1.6",
        "drupal/paragraphs": "^1.7",
        "drupal/pathauto": "^1.0@beta",
        "drupal/publication_date": "^2.0@beta",
        "drupal/redirect": "^1.0@alpha",
        "drupal/scheduled_publish": "^3.1",
        "drupal/search_api": "^1.0@beta",
        "drupal/select2": "^1.12",
        "drupal/simple_sitemap": "^3.1",
        "drupal/slick": "^2.0@rc",
        "drupal/slick_paragraphs": "^2.0",
        "drupal/twig_tweak": "^2.1",
        "drupal/viewsreference": "^2.0@alpha",
        "drupal/webform": "^6.0",
        "drush/drush": "~10.3.6",
        "kint-php/kint": "^3.3",
        "npm-asset/select2": "^4.0",
        "npm-asset/slick-carousel": "^1.7",
        "oomphinc/composer-installers-extender": "^2.0",
        "platformsh/config-reader": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "~9.1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "desandro/masonry",
                "version": "4.2.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/desandro/masonry/archive/v4.2.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "blazy"
                },
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    ],
    "config": {
        "discard-changes": true,
        "sort-packages": true
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "gitignore": false,
            "symlink": false,
            "allowed-packages": [
                "drupal/core"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": "assets/.editorconfig",
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/default/settings.local.php": false
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/slick": [
                "npm-asset/slick-carousel"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {}
    }
}
